@model M0502.Models.Category

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UploadPhoto</title>
</head>
<body>
    @using (Html.BeginForm("UploadPhoto", "Category", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <textarea name="Description" rows="20" cols="80"></textarea>

        <input type="file" name="file" />

        <input type="submit" value="Ok" />
    }
</body>
</html>
